{
    "config": {
      "application": "Mixer",
      "application_ru_RU": "Смешивание сигналов",
      "category": [ "Applications" ],
      "parameters_template": [
          { "name": "Input Seismic2D Names", "inputDataType": "SeismicDataGeoms2DNames", "collectChildren": true, "type": "ListView" },
          { "name": "Seismic2DGeometries", "inputDataType": "SeismicDataGeoms2DNames", "type": "CustomStringList" },
          { "name": "Input Seismic3D Names", "inputDataType": "SeismicDataGeoms3DCubes", "type": "ListView" },
          { "name": "New Name", "type": "LineEdit", "default": "mix" },
       {
          "name": "shift",
          "type": "SpinBox",
          "title": "shift",
          "default": 1,
          "min": 0,
          "max": 10
        },
        {
          "name": "halfwin_traces",
          "type": "SpinBox",
          "title": "halfwin traces",
          "default": 1,
          "min": 0,
          "max": 5
        },
        {
          "name": "attenuations",
          "type": "DoubleSpinBox",
          "title": "attenuations",
          "default": 0.2,
          "min": 0,
          "max": 1
        },
        {
          "name": "type",
          "type": "ComboBox",
          "title": "type",
          "default": "square",
          "items": ["square","cross"]
        },
        { "name": "Threads Count", "type": "SpinBox", "min": 1, "max": 100, "default": 10 },
        {"name": "margin", "type": "CustomInt", "default": 1}
      ]
    }
  }
