{
  "config": {
    "application": "Zcube",
    "application_ru_RU": "Zcube",
    "category": [ "Applications" ],
    "parameters_template": [
        { "name": "Horizon", "inputDataType": "HorizonsNames", "type": "ListView" },
        { "name": "Input Seismic3D Names", "inputDataType": "SeismicDataGeoms3DCubes|SeismicDataGeoms3DNames", "collectChildren": true, "type": "ListView" },
        { "name": "New Name", "type": "LineEdit", "default": "Result" },
        {
          "name": "mode",
          "type": "ComboBox",
          "title": "Mode",
          "default": "proportional",
          "items": ["proportional","From_Top","From_Bottom"]
        },
    {
          "name": "top_shift",
          "type": "DoubleSpinBox",
          "title": "Сдвиг от верхнего горизонта, мс",
          "default": 0,
          "min": -100,
          "max": 100
        },

    {
          "name": "top_bottom",
          "type": "DoubleSpinBox",
          "title": "Сдвиг от нижнего горизонта, мс",
          "default": 0,
          "min": 0,
          "max": 100
        },
    {
          "name": "num_worker",
          "type": "SpinBox",
          "title": "Количество процессов",
          "default": 16,
          "min": 1,
          "max": 120
        },
    {     "name": "chank_size",
          "type": "SpinBox",
          "title": "Размер чанка",
          "default": 150,
          "min": 50,
          "max": 500 }
        
    ]
  }
}
