{
  "config": {
    "application": "cubeHorizontsCalculation",
    "category": [ "Applications" ],
    "parameters_template": [
        { "name": "Horizon", "inputDataType": "HorizonsNames", "type": "LineEdit" },
        { "name": "Input Seismic3D Names", "inputDataType": "SeismicDataGeoms3DCubes", "type": "ListView" },
        { "name": "New Name", "type": "LineEdit", "default": "Result" },
        {
          "name": "attributes",
          "type": "ListView",
          "title": "Attributes",
          "singleSelection": false,
          "items": ["Amplitude","Energy","Effective_amp","Abs_a_div_effective_amp","Pow_a_div_effective_amp",
          "autocorrelation_period","spectral_energy","mean_freq","signal_compression","left_spectral_area","right_spectral_area",
        "absorption_Ssw_Sw","absorption_Ssw_Sww"]
        },
    {
          "name": "radius",
          "type": "DoubleSpinBox",
          "title": "Полуокно интервала, мс",
          "default": 32,
          "min": 0.1,
          "max": 100
        },
    {
          "name": "distance_up",
          "type": "DoubleSpinBox",
          "title": "Расстояние до верхней границы, мс",
          "default": 10,
          "min": 0.1,
          "max": 100
        },

    {
          "name": "distance_down",
          "type": "DoubleSpinBox",
          "title": "Расстояние до нижней границы, мс",
          "default": 10,
          "min": 0.1,
          "max": 100
        },
    {
          "name": "min_freq",
          "type": "DoubleSpinBox",
          "title": "Нижняя частота, Гц",
          "default": 0,
          "min": 0.1,
          "max": 10000
        },
    {
          "name": "max_freq",
          "type": "DoubleSpinBox",
          "title": "Верхняя частота, Гц",
          "default": 125,
          "min": 0.1,
          "max": 10000
        },
    {
          "name": "intermediate_freq",
          "type": "DoubleSpinBox",
          "title": "Опорная частота, Гц",
          "default": 50,
          "min": 0.1,
          "max": 10000
        }
        
    ]
  }
}